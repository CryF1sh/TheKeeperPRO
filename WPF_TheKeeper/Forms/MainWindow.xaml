<Window x:Class="WPF_TheKeeper.Forms.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_TheKeeper.Forms"
        mc:Ignorable="d"
        Title="ListWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Grid Background="Orange"></Grid>
        <Grid Background="Orange" Grid.Row="2"></Grid>
        <Frame Grid.Row="1" x:Name="MainFrame"></Frame>
    </Grid>
</Window>
